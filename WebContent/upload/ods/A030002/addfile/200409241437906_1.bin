CREATE VIEW dbo.v_history_table
AS
SELECT   a.ah_id, a.au_id, b.ap_id, c.at_id, a.s_day, a.s_time, a.class, a.subject, a.content, 
                a.result, a.other_customer, a.file_name, a.file_size, a.umask, a.regi_date, a.flag, 
                b.name AS pname, c.name, d.name AS uname
FROM      dbo.history_table a INNER JOIN
                dbo.company_table b ON a.ap_id = b.ap_id INNER JOIN
                dbo.customer_table c ON a.at_id = c.at_id INNER JOIN
                dbo.user_table d ON a.au_id = d.id

GO
SET QUOTED_IDENTIFIER OFF 
GO
SET ANSI_NULLS ON 
GO

